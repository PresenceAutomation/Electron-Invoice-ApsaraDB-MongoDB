{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/mac/ntally.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . 'Smart Tally' --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/win/ntally.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Smart Tally\"",
    "package-linux": "electron-packager . 'Smart Tally' --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/png/ntally.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.8.4",
    "electron-packager": "^11.2.0",
    "electron-rebuild": "^1.7.3"
  }
}
